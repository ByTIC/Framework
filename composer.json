{
  "name": "ByTIC/Framework",
  "description": "Php Framework",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/ByTIC/RequestDetective.git"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\": "src/"
    },
    "classmap": [
      "src"
    ],
    "files": [
      "src/functions/common.php",
      "src/functions/input.php",
      "src/functions/flash.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Tests\\": "tests/",
      "Nip\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": ">=5.5.0",
    "bytic/request-detective": "^1.0",
    "container-interop/container-interop": "^1.1",
    "nesbot/carbon": "^1.21",
    "maximebf/debugbar": "^1.13",
    "html2text/html2text": "^4.0",
    "sendgrid/sendgrid": "^5.0",
    "swiftmailer/swiftmailer": "^5.4",
    "symfony/http-foundation": "^3.1",
    "symfony/http-kernel": "^3.1",
    "monolog/monolog": "^1.20",
    "filp/whoops": "^2.1",
    "ezyang/htmlpurifier": "^4.9"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "phpunit/phpunit": "^6.2",
    "mockery/mockery": "^0.9"
  }
}

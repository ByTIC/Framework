{
  "name": "bytic/framework",
  "type": "library",
  "description": "Php Framework",
  "homepage": "https://github.com/ByTIC/request-detective",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\": "src"
    },
    "classmap": [
      "src"
    ],
    "files": [
      "src/functions/application.php",
      "src/functions/application_namespaced.php",
      "src/functions/common.php",
      "src/functions/input.php",
      "src/functions/flash.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Tests\\": "tests/",
      "Nip\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.0",
    "psr/container": "^1.0",
    "bytic/request-detective": "^1.0",
    "http-interop/http-middleware": "^0.4",
    "league/container": "^2.3",
    "nesbot/carbon": "^1.21",
    "maximebf/debugbar": "^1.13",
    "html2text/html2text": "^4.0",
    "sendgrid/sendgrid": "^5.0",
    "swiftmailer/swiftmailer": "^5.4",
    "symfony/http-foundation": "^3.1",
    "symfony/http-kernel": "^3.1",
    "monolog/monolog": "^1.20",
    "filp/whoops": "^2.1",
    "vlucas/phpdotenv": "^2.4",
    "symfony/finder": "^3.2",
    "symfony/debug": "^3.2",
    "symfony/filesystem": "^3.2",
    "league/flysystem": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "squizlabs/php_codesniffer": "^2.0",
    "codeception/mockery-module": "^0.2"
  }
}

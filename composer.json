{
  "name": "bytic/framework",
  "type": "library",
  "description": "Php Framework",
  "homepage": "https://github.com/ByTIC/request-detective",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "Nip\\": "src"
    },
    "classmap": [
      "src"
    ],
    "files": [
      "src/functions/application.php",
      "src/functions/application_namespaced.php",
      "src/functions/common.php",
      "src/functions/input.php",
      "src/functions/flash.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Nip\\Tests\\": "tests/",
      "Nip\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": "^7.0",
    "bytic/application": "^1.0",
    "bytic/autoloader": "^1.0",
    "bytic/cache": "^1.0",
    "bytic/controllers": "^1.0",
    "bytic/collections": "^1.0",
    "bytic/config": "^1.0",
    "bytic/container": "^1.0",
    "bytic/database": "^1.0",
    "bytic/debug": "^1.0",
    "bytic/debugbar": "^1.0",
    "bytic/dispatcher": "^1.0",
    "bytic/form": "^1.0",
    "bytic/filesystem": "^1.0",
    "bytic/helpers": "^1.0",
    "bytic/http": "^1.0",
    "bytic/inflector": "^1.0",
    "bytic/logger": "^1.0",
    "bytic/locale": "^1.0",
    "bytic/mail": "^1.0",
    "bytic/orm": "^1.0",
    "bytic/profiler": "^1.0",
    "bytic/request-detective": "^1.0",
    "bytic/router": "^1.0",
    "bytic/translation": "^1.0",
    "bytic/utility": "^1.0",
    "bytic/view": "^1.0",
    "nesbot/carbon": "^1.21",
    "filp/whoops": "^2.1",
    "vlucas/phpdotenv": "^2.4",
    "symfony/finder": "^4.0",
    "symfony/filesystem": "^3.2",
    "ezyang/htmlpurifier": "^4.9"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "phpunit/phpunit": "^7.2",
    "mockery/mockery": "^1.1"
  }
}
